\n            <button class=\"page-item active\">1</button>\r\n            <button class=\"page-item\">2</button>\r\n            <button class=\"page-item\">3</button>\r\n            <button class=\"page-item\">下一页</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- 预约未到 -->\r\n      <div class=\"tab-pane\" id=\"tab2\">\r\n        <table>\r\n          <thead>\r\n            <tr>\r\n              <th>序号</th>\r\n              <th>姓名</th>\r\n              <th>手机号</th>\r\n              <th>预约时间</th>\r\n              <th>就诊类型</th>\r\n              <th>医生</th>\r\n              <th>挂号科室</th>\r\n              <th>症状描述</th>\r\n              <th>预约备注</th>\r\n              <th>操作</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(w,i) of yypats\" :key=\"i\">\r\n              <td>{{w.pid}}</td>\r\n              <td>{{w.pname}}</td>\r\n              <td>{{w.tel}}</td>\r\n              <td>{{w.data}}</td>\r\n              <td>{{w.cfz}}</td>\r\n              <td>{{w.ename}}</td>\r\n              <td>{{w.dname}}</td>\r\n              <td>{{w.details}}</td>\r\n              <td>{{w.txt}}</td>\r\n              <td><button>查看</button></td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n        <div class=\"pageNav\">>\r\n          <div class=\"pagination\">\r\n            <button class=\"page-item disabled\">上一页</button>\r\n            <button class=\"page-item active\">1</button>\r\n            <button class=\"page-item\">2</button>\r\n            <button class=\"page-item\">3</button>\r\n            <button class=\"page-item\">下一页</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <!-- 待收费 -->\r\n      <!-- 已收费 -->\r\n    </div>\r\n  </div>\r\n</div>\r\n\r\n</template>\r\n<script>\r\nexport default {\r\n  data(){return {\r\n    watings:[\r\n      {pid:1,pname:\"张明\",data:\"2019-07-06 14:23:59\",cfz:\"初诊\",ename:\"张医生\",status:\"0\",txt:\"gg\"},\r\n      {pid:2,pname:\"张明\",