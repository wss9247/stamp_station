tab-container-item>\r\n      <mt-tab-container-item id=\"find\">发现面板</mt-tab-container-item>\r\n      <mt-tab-container-item id=\"me\">我的面板</mt-tab-container-item>\r\n    </mt-tab-container>\r\n    <!-- 底部导航条子组件 -->\r\n    <mt-tabbar v-model=\"active\" fixed>\r\n      <!-- v-for循环遍历出所有的底部导航选项 -->\r\n      <mt-tab-item v-for=\"(item,i) of items\" :key=\"i\" :id=\"item.ctitle\">\r\n        <!-- 引入导航小图标子组件，并传递数据 -->\r\n        <btmBarImg :focused=\"item.focused\" :selectedImg=\"item.selectedImg\" :normalImg=\"item.normalImg\"/>\r\n        <!-- 每个导航按钮标题 -->\r\n        {{item.title}}\r\n      </mt-tab-item>\r\n    </mt-tabbar>\r\n    <!-- 调用自定义函数 -->\r\n   {{ iconUrl}}\r\n  </div>\r\n</template>\r\n<script>\r\nimport messageList from './messageList.vue';\r\nimport btmBarImg from './btmBarImg.vue'\r\nexport default {\r\n  data(){return{\r\n    active:\"weixin\",\r\n    items:[\r\n      {ctitle:\"weixin\",title:\"微信\",focused:true,selectedImg:require('../../../assets/ic_weixin_selected.png'),normalImg:require('../../../assets/ic_weixin_normal.png')},\r\n      {ctitle:\"contacts\",title:\"通讯录\",focused:false,selectedImg:require('../../../assets/ic_contacts_selected.png'),normalImg:require('../../../assets/ic_contacts_normal.png')},\r\n      {ctitle:\"find\",title:\"发现\",focused:false,selectedImg:require('../../../assets/ic_find_selected.png'),normalImg:require('../../../assets/ic_find_normal.png')},\r\n      {ctitle:\"me\",title:\"我\",focused:false,selectedImg:require('../../../assets/ic_me_selected.png'),normalImg:require('../../../assets/ic_me_normal.png')},\r\n    ]    \r\n  }},\r\n  computed:{\r\n    iconUrl(){  //自定义函数，用于判断哪个导航按钮被选中，选中则设置focused=true\r\n      for(var i=0;i<this.items.length;i++){        \r\n        this.items[i].focused=false;\r\n        if(this.active==this.items[i].ctitle){\r\n          this.items[i].focused=true;\r\n        }\r\n      }      \r\n    }\r\n  },\r\n  components:{messageList,btmBarImg}\r\n}\r\n</script>\r\n<style>\r\n.container{margin-top:8px;}\r\n.mint-tabbar > .mint-tab-item{color:#666;}\r\n.mint-tabbar > .mint-tab-item.is-selected{\r\n  color:#45c018!important;\r\n}\r\n\r\n</style>\r\n\r\n"]}]}