    methods:{\r\n        openPrompt(){\r\n            this.$messagebox.prompt(\"请输入年龄\")\r\n            .then((value)=>{\r\n                console.log(value);\r\n            }).catch(err=>{\r\n                console.log(err)\r\n            });\r\n        },\r\n        openConfirm(){\r\n            //功能：显示一个确认的消息框\r\n            this.$messagebox.confirm(\"是否删除指定的数据\")\r\n            .then(action=>{//用户选择了确认的回调函数\r\n                console.log(\"确认\");\r\n            })\r\n            .catch(action=>{\r\n                console.log(\"取消\")\r\n            })\r\n            \r\n        },\r\n        openAlert(){\r\n            // 标准方式显示提示框\r\n            // this.$messagebox({\r\n            //     title:\"消息\",\r\n            //     message:\"执行成功\"\r\n            // })\r\n\r\n            // 简写\r\n            this.$messagebox(\"简写\",\"执行成功\")\r\n        },\r\n        openToast(){\r\n            // 功能：此函数显示完成mint-ui消息提示框\r\n            this.$toast({\r\n                message:\"操作成功\",\r\n                duration:\"1000\",\r\n                position:\"top\",\r\n                // 使用字体名称       图标名称\r\n                iconClass:\"iconfont icon-icon-test\"\r\n            });\r\n            console.log(this);\r\n        }\r\n    },\r\n}\r\n\r\n</script>\r\n<style>\r\n/* 当前组件专有的样式 */\r\n</style>\r\n\r\n\r\n"]}]}